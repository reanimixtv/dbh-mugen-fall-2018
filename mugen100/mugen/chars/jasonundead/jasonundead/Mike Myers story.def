; Screen color
clearcolor = 0,0,0
; Overlay Anims (from 0 to 9)
layerall.pos = 160,-30   ;Default position for all layers
layer0.anim =  0       ;Anim action number
layer0.offset = 0,32   ;Position offset
layer0.starttime = 0   ;Time to start display
layer1.anim =  1
layer1.offset = 0,176

; Music
bgm = halloween.mp3     ;Name of music file to play
bgm.loop = 1

; Total time
end.time = 1860


